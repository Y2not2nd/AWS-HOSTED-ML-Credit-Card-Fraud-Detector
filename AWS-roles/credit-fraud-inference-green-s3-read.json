{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListBucketForModelAndRelease",
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": ["arn:aws:s3:::mlops-model-store-prod"],
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "bentoml/*",
            "releases/credit-fraud/*"
          ]
        }
      }
    },
    {
      "Sid": "ReadModels",
      "Effect": "Allow",
      "Action": ["s3:GetObject"],
      "Resource": [
        "arn:aws:s3:::mlops-model-store-prod/bentoml/*"
      ]
    },
    {
      "Sid": "ReadReleasePointers",
      "Effect": "Allow",
      "Action": ["s3:GetObject"],
      "Resource": [
        "arn:aws:s3:::mlops-model-store-prod/releases/credit-fraud/*"
      ]
    }
  ]
}
